<!DOCTYPE generatorConfiguration 
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" 
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
    <generatorConfiguration >
    <!-- 设置mysql驱动路径 -->
    <classPathEntry location="D:/Java/maven/repo/mysql/mysql-connector-java/5.1.9/mysql-connector-java-5.1.9.jar" />
    <!-- 此处指定生成针对MyBatis3的DAO -->
  
      <context id="context1"  targetRuntime="MyBatis3">
            <!--	<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">  
		     <property name="searchString" value="Example$" />  
		     <property name="replaceString" value="NewName" />  
		</plugin>  -->
      
      	<!-- 关闭注解信息 -->
		<commentGenerator>  
        	<property name="suppressAllComments" value="true" />  
		</commentGenerator>
        <!-- jdbc连接信息 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/cw?useUnicode=true&amp;characterEncoding=UTF-8"
        userId="root" password="root" />
        <!-- 生成model对象 -->
        <javaModelGenerator targetPackage="com.lee.model" targetProject="cw" />
        <!-- 生成Mapper.xml -->
        <sqlMapGenerator targetPackage="mapper" targetProject="cw/src/main/resources" />  
        <!-- 生成DAO的类文件以及配置文件 -->
        <javaClientGenerator targetPackage="mapper" targetProject="cw/src/main/resources" type="XMLMAPPER" />
